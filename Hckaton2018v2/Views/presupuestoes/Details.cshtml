@model Hckaton2018v2.presupuesto

<section class="wellcome_area clearfix" id="home">
    <div class="container h-100">
        <div class="row h-50 align-items-center">
            <div class="col-12 col-md">
                <div>
                    <div class="page-header text-center">
                        <h1 style="color:#f3f3ff">Presupuestos</h1>
                    </div>
                    <hr />
                    <dl class="dl-horizontal">
                        <dt class="text-capitalize font-weight-bold">
                            Presupuesto Total
                        </dt>

                        <dd class="text-light">
                            @Html.DisplayFor(model => model.prepTotal)
                        </dd>

                        <dt class="text-capitalize font-weight-bold">
                            @Html.DisplayNameFor(model => model.estado.nombre)
                        </dt>

                        <dd class="text-light">
                            @Html.DisplayFor(model => model.estado.nombre)
                        </dd>

                    </dl>
                </div>
                <p class="h5">
                    @Html.ActionLink("Editar", "Edit", new { id = Model.idPresupuesto }) |
                    @Html.ActionLink("Regresar", "Index")
                </p>
            </div>
        </div>
    </div>
</section>